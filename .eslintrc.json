{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "max-len": [
      2,
      {
        "code": 80,
        "ignorePattern": "^import\\W.*|"
      }
    ]
  },
  "env": {
    "jest": true
  }
}
