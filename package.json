{
  "name": "bakuhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "commit": "git-cz",
    "kill": "npx kill-port 3000",
    "restart": "rm -r .next && npm run kill && npm run dev",
    "prisma-seed": "npx prisma db seed",
    "prisma-schema-reformat": "npx prisma format",
    "prisma-schema-update": "npx prisma generate",
    "db": "npx prisma studio",
    "validate-type-graphql-schema": "npm run prisma-schema-update && npm run restart",
    "prisma-schema-push-to-db": "npx prisma db push",
    "prisma-redeploy-db-structure": "npm run prisma-schema-reformat && npm run prisma-schema-update && npm run prisma-schema-push-to-db"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "~3.10.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@types/lodash": "^4.14.180",
    "apollo-server-micro": "^3.6.4",
    "aws-sdk": "^2.1092.0",
    "graphql": "^15.5.1",
    "graphql-relay": "^0.10.0",
    "lodash": "^4.17.21",
    "micro-cors": "^0.1.1",
    "next": "12.1.0",
    "next-auth": "^4.3.1",
    "nodemailer": "^6.7.2",
    "puppeteer": "^13.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "1.1.1",
    "typegraphql-prisma": "^0.19.1"
  },
  "devDependencies": {
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^17.0.21",
    "@types/react": "17.0.39",
    "@types/react-redux": "^7.1.23",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "git-cz": "^4.8.0",
    "prisma": "~3.10.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
