{
  "name": "bakuhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "commit": "git-cz",
    "release": "release-it",
    "kill": "npx kill-port 3000",
    "restart": "rm -r .next && npm run kill && npm run dev",
    "prisma-seed": "npx prisma db seed",
    "prisma-schema-reformat": "npx prisma format",
    "prisma-schema-update": "npx prisma generate",
    "db": "npx prisma studio",
    "update-schema-gql": "curl --request POST \\\n  --header 'content-type: application/json' \\\n  --url http://localhost:3000/api/graphql \\\n  --data '{\"query\":\"query { __typename }\"}'",
    "analyze": "cross-env ANALYZE=true npm run build",
    "validate-type-graphql-schema": "npm run prisma-schema-update && npm run restart",
    "prisma-schema-push-to-db": "npx prisma db push",
    "prisma-redeploy-db-structure": "npm run prisma-schema-reformat && npm run prisma-schema-update && npm run prisma-schema-push-to-db && update-schema-gql",
    "server": "cd && cd MemoryPalaces/WebstormProjects/typegraphql-prisma/examples/3-picking-actions/ && npm run go"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
