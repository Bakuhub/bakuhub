    "@prisma/client": "^3.11.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server-micro": "^3.6.4",
    "aws-sdk": "^2.1092.0",
    "axios": "^0.26.1",
    "cross-env": "^7.0.3",
    "graphql": "^15.5.1",
